<% include partials/header %>

<h1>Edit Blog</h1>
<form action="/blogs/<%= blog._id %>?_method=PUT" method="post">
    <label>Title:</label>
    <input type="text" name="title" value="<%= blog.title %>"><br>
    <label>Image:</label>
    <input type="text" name="image" value="<%= blog.image %>"> <br>
    <label>Body:</label>
    <textarea name="body"cols="30" rows="10">
        <%= blog.body %>
    </textarea>
    <button>Send</button>
</form>

<% include partials/footer %>

